{
  "name": "blur-extension",
  "author": "Aleksandr Hovhannisyan",
  "license": "MIT",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "pnpm exec tsc",
    "copy-assets": "rsync -av --exclude 'tsconfig.json' --exclude 'src' --exclude 'dist' --exclude node_modules --exclude .gitignore --exclude .git ./ dist/",
    "build": "pnpm clean && pnpm compile && pnpm copy-assets"
  },
  "keywords": ["chrome extension", "blur text"],
  "packageManager": "pnpm@8.15.9+sha256.daa27a0b541bc635323ff96c2ded995467ff9fe6d69ff67021558aa9ad9dcc36",
  "devDependencies": {
    "@types/chrome": "^0.0.272",
    "typescript": "^5.6.2"
  }
}

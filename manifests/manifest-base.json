{
  "manifest_version": 3,
  "name": "Blur",
  "description": "Blur any highlighted text or media on a page.",
  "version": "1.0.2",
  "permissions": ["contextMenus", "scripting", "activeTab", "storage"],
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "action": {
    "default_popup": "src/popup.html"
  }
}
